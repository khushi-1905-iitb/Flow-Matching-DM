#include <bits/stdc++.h>
using namespace std;
int main()
{ int t;
std::cin>>t;
while (t--)
{ long long n,k;
cin>>n>>k;
vector <long long> a(n);
for (int i=0; i<n; i++)
 std::cin>>a[i];
 vector <long long> sorted=a;
 sort(sorted.begin(), sorted.end());
 if (sorted==a || k>1)
   std::cout<<"yes"<<endl;
 else 
   cout<<"no"<<endl;
}
return 0;

}
