#include <bits/stdc++.h>
using namespace std;

bool see(string s, string x){
    if (x.size()<s.size()) return false;
    else{
        for (int i=0; i<x.size()-s.size() +1; i++){
            if (x.substr(i, s.size())==s) return true;
        }
           return false;    }
        }

int main()
{ int t;
std::cin>>t;
while (t--)
{ int n,m;
    cin>>n>>m;
    string s, x;
    cin>>x>>s;
    string x0 =x;
    string x1=x0+x0;
     string x2=x1+x1;
      string x3=x2+x2;
       string x4=x3+x3;
        string x5=x4+x4;
        if (see(s,x0)) {cout<<"0"<<endl;}
        else if (see(s,x1)) cout<<"1"<<endl;
        else if (see(s,x2)) cout<<"2"<<endl;
        else if (see(s,x3)) cout<<"3"<<endl;
        else if (see(s,x4)) cout<<"4"<<endl;
        else if (see(s,x5)) cout<<"5"<<endl;
        else cout<<"-1"<<endl;

}
}
